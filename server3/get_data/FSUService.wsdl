<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions targetNamespace="http://FSUService.chinatelecom.com"
                  xmlns:apachesoap="http://xml.apache.org/xml-soap"
                  xmlns:impl="http://FSUService.chinatelecom.com"
                  xmlns:intf="http://FSUService.chinatelecom.com"
                  xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema">

   <wsdl:message name="invokeRequest">

      <wsdl:part name="xmlData" type="soapenc:string"/>

   </wsdl:message>

   <wsdl:message name="invokeResponse">

      <wsdl:part name="invokeReturn" type="soapenc:string"/>

   </wsdl:message>

   <wsdl:portType name="FSUService">

      <wsdl:operation name="invoke" parameterOrder="xmlData">

         <wsdl:input message="impl:invokeRequest" name="invokeRequest"/>

         <wsdl:output message="impl:invokeResponse" name="invokeResponse"/>

      </wsdl:operation>

   </wsdl:portType>

   <wsdl:binding name="FSUServiceSoapBinding" type="impl:FSUService">

      <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>

      <wsdl:operation name="invoke">

         <wsdlsoap:operation soapAction=""/>

         <wsdl:input name="invokeRequest">

            <wsdlsoap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="http://FSUService.chinatelecom.com" use="encoded"/>

         </wsdl:input>

         <wsdl:output name="invokeResponse">

            <wsdlsoap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="http://FSUService.chinatelecom.com" use="encoded"/>

         </wsdl:output>

      </wsdl:operation>

   </wsdl:binding>

   <wsdl:service name="FSUServiceService">

      <wsdl:port binding="impl:FSUServiceSoapBinding" name="FSUService">

         <wsdlsoap:address location="http://localhost:8080/services/FSUService"/>

      </wsdl:port>

   </wsdl:service>

</wsdl:definitions>
